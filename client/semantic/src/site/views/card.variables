/*******************************
    User Variable Overrides
*******************************/

/* Card */
@background: @white;
@borderRadius: 1px;
@margin: 1em 0em;
@minHeight: 0px;
@padding: 0em;
@width: 366px;
@borderWidth: 1px;
@borderShadow: 0px 0px 0px @borderWidth @solidBorderColor;
@boxShadow:
  @shadowBoxShadow,
  @borderShadow
;
@border: none;