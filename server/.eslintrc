{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": [ "off", "always" ],
    "quotes": [ "error", "single" ],
    "no-console": [ "off" ]
  }
}
